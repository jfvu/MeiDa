<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical"
    >
    <View
        android:layout_width="match_parent"
        android:layout_height="20px"
        android:background="#ffffff"/>
    <RelativeLayout
        android:id="@+id/rl_withdraw21"
        android:layout_width="match_parent"
        android:layout_height="44px">
        <ImageView
            android:id="@+id/img_back_withdraw2_activity"
            android:layout_centerVertical="true"
            android:layout_width="9px"
            android:layout_height="16px"
            android:layout_margin="12px"
            android:src="@mipmap/back3x"/>
        <TextView
            android:layout_centerInParent="true"
            android:textSize="18px"
            android:gravity="center"
            android:layout_width="37px"
            android:layout_height="25px"
            android:text="提现"
            />
        <ImageView
            android:id="@+id/img_menu_withdraw2_activity"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:src="@mipmap/diandiandian3x"
            android:layout_alignParentRight="true"
            android:layout_marginRight="12px"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="8px"
        android:background="#EEEEEE"/>
    <RelativeLayout
        android:id="@+id/rl_withdraw2"
        android:layout_width="match_parent"
        android:layout_height="56px"
        >
        <TextView
            android:layout_centerVertical="true"
            android:layout_width="68px"
            android:layout_height="22px"
            android:text="钱包余额"
            android:textSize="16px"
            android:layout_marginLeft="12px"
            android:textColor="@color/black"/>
        <TextView
            android:layout_centerVertical="true"
            android:layout_width="64px"
            android:layout_height="22px"
            android:layout_alignParentRight="true"
            android:text="1234.00"
            android:layout_marginRight="12px"
            android:textColor="@color/blue"
            android:textSize="16px"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="44px"
        android:background="#EEEEEE"
        >
        <TextView
            android:layout_width="144px"
            android:layout_height="20px"
            android:layout_marginLeft="12px"
            android:text="可提现金额1234.00元"
            android:layout_centerVertical="true"
            android:textSize="14px"/>
        <TextView
            android:layout_width="59px"
            android:layout_height="20px"
            android:text="全部提现"
            android:textSize="14px"
            android:layout_marginRight="12px"
            android:layout_centerVertical="true"
            android:textColor="@color/blue"
            android:layout_alignParentRight="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="56px"
        >
        <TextView
            android:layout_width="68px"
            android:layout_height="22px"
            android:text="提现金额"
            android:textSize="16px"
            android:textColor="@color/black"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12px"/>
        <EditText
            android:layout_width="148px"
            android:layout_height="20px"
            android:layout_alignParentRight="true"
            android:hint="输入金额必须大于10元"
            android:textColor="@color/blue"
            android:textSize="14px"
            android:layout_centerVertical="true"
            android:layout_marginRight="12px"
            android:background="@null"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#EEEEEE">
        <TextView
            android:id="@+id/next"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="295px"
            android:background="@mipmap/tijiaorenwu3x"
            android:layout_width="220px"
            android:layout_height="48px"
            android:text="下一步"
            android:textColor="@color/blue"
            android:textSize="18px"
            android:gravity="center"/>
    </RelativeLayout>
</LinearLayout>
